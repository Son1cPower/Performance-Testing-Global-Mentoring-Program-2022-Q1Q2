log.info(&quot;MM = &quot;+ MM)

def DD = split_data[2]
log.info(&quot;DD = &quot;+ DD)

def TIME = split_data[3]
log.info(&quot;TIME = &quot;+ TIME)


vars.put(&quot;YYYY&quot;,&quot;${YYYY}&quot;)
vars.put(&quot;MM&quot;,&quot;${MM}&quot;)
vars.put(&quot;DD&quot;,&quot;${DD}&quot;)
vars.put(&quot;TIME&quot;,&quot;${TIME}&quot;)


    log.info(&quot;------ end ----------&quot;);</stringProp>
